{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "salesorders-config1",
      "cwd": "${workspaceFolder}/salesorders",
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": [
        "--open",
        "index.html"
      ],
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/salesorders\"}",
        "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/b1s\",\"url\":\"http://sam-mobile.cloud:443\",\"destination\":\"SAM_B1ServiceLayer\"}]",
        "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false"
      }
    }
  ]
}